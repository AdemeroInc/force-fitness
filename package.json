{
  "name": "force-fitness",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "echo \"Add tests here\"",
    "deploy:dev": "npm run build && firebase use development && firebase deploy --only apphosting",
    "deploy:staging": "npm run build && firebase use staging && firebase deploy --only apphosting",
    "deploy:prod": "npm run build && firebase use production && firebase deploy --only apphosting",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "deploy:all": "npm run build && firebase deploy --only apphosting,firestore:rules,storage:rules",
    "emulators": "firebase emulators:start",
    "clean": "rm -rf .next"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@firebase/auth": "^1.11.0",
    "@firebase/firestore": "^4.9.0",
    "@firebase/storage": "^0.14.0",
    "@google-cloud/aiplatform": "^5.0.0",
    "@google-cloud/secret-manager": "^6.1.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "canvas": "^3.1.2",
    "date-fns": "^4.1.0",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^12.23.7",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.4.3",
    "openai": "^5.10.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-firebase-hooks": "^5.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
